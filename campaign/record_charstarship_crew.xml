				</parameters>
				<tooltip>
					<textres>charstarship_tooltip_crew_role</textres>
				</tooltip>
				<script>
					function onInit()
						super.onInit();
						if not DB.isOwner(window.getDatabaseNode()) then
							setEnabled(false);
						else
							updateActions();
							DB.addHandler(DB.getPath(window.getDatabaseNode(), "role"), "onUpdate", updateActions);
						end
					end

					function updateActions()
						removeActions();
						buildActions();
					end
					function removeActions()
						window.actions.deleteActions();
						window.minoractions.deleteActions();	
					end
					function buildActions()
						if getStringValue() == "" then
							window.updateActionHeaders("Captain");
							window.buildRoleActions("Captain");
							window.actionsframe.setAnchoredHeight("300");
						else
							window.updateActionHeaders(getStringValue());
							window.buildRoleActions(getStringValue());
							if getStringValue() == "Pilot" then	
								window.actionsframe.setAnchoredHeight("300");
							elseif getStringValue() == "Gunner" then	
								window.actionsframe.setAnchoredHeight("450");
							else
								window.actionsframe.setAnchoredHeight("170");
							end
						end
					end
				</script>
