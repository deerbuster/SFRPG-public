<root>
	<windowclass name="charstarship_notes">
		<sheetdata>
			<frame_char name="notesframe">
				<bounds>15,5,-29,-20</bounds>
			</frame_char>
			<charstarship_label_frametop>
				<anchored to="notesframe" />
				<static textres="charstarship_label_notes" />
				<color>#808080</color>
			</charstarship_label_frametop>
			<stringu name="notes">
				<anchored to="notesframe">
					<top offset="30" />
					<left offset="15" />
					<right offset="-20" />
					<bottom offset="-15" />
				</anchored>
				<multilinespacing>20</multilinespacing>
			</stringu>
			<scrollbar>
				<anchored to="notes" />
				<target>notes</target>
			</scrollbar>
		</sheetdata>
		
	</windowclass>

</root>
