<root>	
	<template name="close_imagebox">
		<close>
			<anchored>
				<top offset="5" />
				<right offset="-1" />
			</anchored>
		</close>
	</template>
	<template name="help_imagebox">
		<help>
			<anchored>
				<top offset="5" />
				<right offset="-25" />
			</anchored>
		</help>
	</template>
	<template name="resize_imagebox">
		<genericcontrol>
			<anchored height="18" width="18">
				<right offset="-14" />
				<bottom offset="-10" />
			</anchored>
			<icon>window_resize_filled</icon>
			<disabled />
		</genericcontrol>
	</template>
	<template name="close_storybox" merge="join">
		<close>
			<anchored>
				<top offset="0" />
				<right offset="-3" />
			</anchored>
		</close>
	</template>	
</root>
